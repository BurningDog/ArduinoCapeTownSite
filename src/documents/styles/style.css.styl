@import 'nib'

a, a:link
	cursor pointer

body
	padding-top 60px

table
	tfoot, tbody
		th
			border-top 1px solid #DDD

.navbar
	min-height 80px

.navbar-brand
	text-indent -9999px
	background-image url('/images/logo.png')
	width 246px
	height 80px

.navbar-inverse
	background-color #09949b
	border-color #D6F4F5
	.navbar-brand
		color white
	.navbar-brand:hover
		color #D6F4F5
	.navbar-brand:focus
		color #D6F4F5
